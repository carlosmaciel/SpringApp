<div class="container" id="container-list-associados">
	
	<div class="panel panel-primary">
		<div class="panel-heading">
			<h3>Lista de Associados</h3>
		</div>
		<div class="alert alert-success alert-dismissible alert-associado"  role="alert">
			Associado criado com sucesso!
			<button type="button" class="close" data-dismiss="alert" aria-label="Close">
				<span aria-hidden="true">&times;</span>
			</button>
		</div>
		<div class="panel-body">
			<table class="table table-striped" id="grid-associados">
				<thead>
					<tr>
						<th width="20%">ID</th>
						<th width="60%">Nome</th>
						<th width="20%">CPF</th>
					</tr>
				</thead>
				<tbody>
				</tbody>
			</table>
			<div>
				<a type="button" class="btn btn-primary btn-md" id="btn-add-associado" href="#">Adicionar Associado</a>
			</div>
		</div>
	</div>

</div>